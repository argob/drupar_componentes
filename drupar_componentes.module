/**
* Implements hook_block_info().
*/
function drupar_componentes_block_info() {
  $blocks = array();
  $blocks['redes_sociales_generico'] = array(
    'info' => t('Redes sociales reutilizable'),
  );
  return $blocks;
}
/**
* Implements hook_block_view().
*/
function drupar_componentes_block_view($delta = '') {
  $node = node_load(arg(1));
  $block = array();
  switch ($delta) {
    case 'redes_sociales_generico':
      $block['subject'] = '';
      $block['content'] = _drupar_componentes_BLOCK_ABC_CONTENT($node);
      break;
  }
  return $block;
}

function _drupar_componentes_BLOCK_ABC_CONTENT($node) {
  $output = t('<div class="section-actions col-md-6 social-share">
      <p>Compartí <br>este servicio</p>
      <ul class="list-inline">
        <li><a href="http://www.facebook.com/sharer/sharer.php?u='.url(current_path(), array('absolute' => TRUE)).'&amp;title='.$node->title.'"><i class="fa fa-facebook"></i></a></li>
        <li><a href="https://twitter.com/share"><i class="fa fa-twitter "></i></a></li>
        <li><a href="https://plus.google.com/share?url='.url(current_path(), array('absolute' => TRUE)).'"><i class="fa fa-google-plus"></i></a></li>
      </ul>
    </div>');
  return $output;
}

/*
* Implements hook_ctools_plugin_directory().
*/

function drupar_componentes_ctools_plugin_directory($owner, $plugin_type) {
   if ($owner == 'ctools' && $plugin_type == 'content_types') {
     return 'plugins/' . $plugin_type;
   }
}

